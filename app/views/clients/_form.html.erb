<%= form_for @client, :url => client_path, :html => { :multipart =>true } do |f| %>
  <%= f.label :title, "Название организации" %><br/>
  <%= f.text_field :title %><br/>
  
  <%= f.label :description, "Описание" %><br/>
  <%= f.text_area :description %><br/>
  
  <%= f.file_field :logo %><br/>
  
  <%= f.submit @client.button_value %>
<% end %>

